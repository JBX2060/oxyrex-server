    for (let i = 0; i < 4; i++) {
      output.push(
        {
          POSITION: [9, 10, 0, (360 / 4) * i, 0, 0],
          TYPE: exports.quadriaticTurret,
        },
        {
          POSITION: [3, 6, 6, (360 / 4) * i, 120, 1],
          TYPE: exports.autoTurret,
        }
      );
    }
    output.push({
      POSITION: [8, 0, 0, 0, 360, 1],
      TYPE: exports.redistributorTurret,
    });
    return output;
  })(),
  BROADCAST_MESSAGE:
    "The Quadriatic may have fallen, but the battle is not yet over...",
  FRAG_SPAWNS: [
    "quadriaticShard",
    "quadriaticShard",
    "quadriaticShard",
