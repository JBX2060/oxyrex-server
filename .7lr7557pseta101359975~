exports.triNuke = {
  PARENT: [exports.bullet],
  LABEL: "Container",
  SHAPE: shapeConfig.triShrapnel,
  INDEPENDENT: true,
  BODY: {
    RANGE: 120,
  },
  MISSILE: true,
  FACING_TYPE: "turnWithSpeed",
  GUNS: (() => {
    let output = [];
    for (let i = 0; i < 3; i++) {
      output.push({
        POSITION: [1, 10, 1, 0, 0, (360 / 3) * i, Infinity],
        PROPERTIES: {
          AUTOFIRE: true,
          SHOOT_SETTINGS: combineStats([g.bullet, g.flank, g.pound]),
          TYPE: [
            exports.nuke,
            {
              PERSISTS_AFTER_DEATH: true,
            },
          ],
          SHOOT_ON_DEATH: true,
        },
      });
    }
    return output;
  })(),
};
