exports.shredder = makeLancer("Shredder", 1, 1.1, 1.2, 1);
for (let i = 7, k = 3; i > 0; i--) {
  exports.shredder.GUNS.push({
    POSITION: [3, 0.05, -55, k, -((20 / 8) * i) - 5, 80, 0],
  });
  k += (3.3 / 8) * 0.9 * 0.75;
}
for (let i = 7, k = 3; i > 0; i--) {
  exports.shredder.GUNS.push({
    POSITION: [3, 0.05, -55, k, (20 / 8) * i + 5, -80, 0],
  });
  k += (3.3 / 8) * 0.9 * 0.75;
}
