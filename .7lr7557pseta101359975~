exports.barstud = (function () {
  exports.actualBarStud = {
    PARENT: [exports.bullet],
    LABEL: "Split",
    SHAPE: ["", 1],
    SYNCS_SKILLS: true,
    GUNS: [
      {
        POSITION: [20.5, 5, 1, -10.25, 0, 180, 0.01],
      },
      {
        POSITION: [7, 10, -0.5, 10, 0, 180, 0.01],
        PROPERTIES: {
          SHOOT_SETTINGS: combineStats([g.bullet, g.barspam]),
          TYPE: [
            exports.bardust,
            {
              PERSISTS_AFTER_DEATH: true,
            },
          ],
          STAT_CALCULATOR: gunCalcNames.swarm,
          AUTOFIRE: true,
          COLOR: 0,
          SKIN: 11,
        },
      },
    ],
  };
  return {
    PARENT: [exports.bullet],
    LABEL: "Split",
    SYNCS_SKILLS: true,
    GUNS: (function () {
      const output = [];
      for (let i = -1; i < 2; i++)
        output.push({
          POSITION: [0, 10.5, 1.2, 7, 0, i * 15, 1e99],
          PROPERTIES: {
            SHOOT_SETTINGS: combineStats([
              g.bullet,
              g.pound,
              g.launcher,
              g.bar,
            ]),
            TYPE: [
              exports.actualBarStud,
              {
                PERSISTS_AFTER_DEATH: true,
              },
            ],
            SHOOT_ON_DEATH: true,
            STAT_CALCULATOR: gunCalcNames.swarm,
            AUTOFIRE: true,
          },
        });
      return output;
    })(),
  };
})();
