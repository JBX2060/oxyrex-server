exports.mothership = {
  PARENT: [exports.genericTank],
  LABEL: "Mothership",
  DANGER: 10,
  SIZE: exports.genericTank.SIZE * (7 / 3),
  SHAPE: 16,
  STAT_NAMES: statnames.drone,
  ACCEPTS_SCORE: true,
  VALUE: 5e5,
  SKILL: [9, 9, 9, 9, 9, 9, 9, 9, 9, 9],
  BODY: bossStats({
    health: 32,
    damage: 0.1,
    speed: 0.5,
  }),
  GUNS: (() => {
    let e = [],
      T = [1];
    for (let e = 1; e < 8.5; e += 0.5) {
      let t = e / 16;
      T.push(t);
    }
    for (let t = 0; t < 16; t++) {
      let S = 22.5 * (t + 1),
        E = {
          MAX_CHILDREN: 2,
          SHOOT_SETTINGS: combineStats([g.drone, g.overseer, g.mothership]),
          TYPE: exports.drone,
          AUTOFIRE: true,
          SYNCS_SKILLS: true,
          STAT_CALCULATOR: gunCalcNames.drone,
          WAIT_TO_CYCLE: true,
        };
      t % 2 == 0 &&
        (E.TYPE = [
          exports.autodrone,
          {
            AI: {
              blind: true,
            },
            LAYER: 10,
            BODY: {
              FOV: 1.5,
            },
          },
        ]);
      let O = {
        POSITION: [4.3, 3.1, 1.2, 8, 0, S, T[t]],
        PROPERTIES: E,
      };
      e.push(O);
    }
    return e;
  })(),
};
