exports.taurusPortal = {
  PARENT: [exports.trap],
  LABEL: "Portal",
  FACING_TYPE: "spin",
  SHAPE: 100,
  CONTROLLERS: ["taurusPortal"],
  GUNS: (() => {
    let output = [];
    for (let i = 0; i < 4; i++)
      output.push({
        POSITION: [1, 8, 1, 0, 0, (360 / 4) * i, Infinity],
        PROPERTIES: {
          SHOOT_SETTINGS: combineStats([g.bullet, g.taurusPortalScaling]),
          TYPE: [
            exports.bullet,
            {
              PERSISTS_AFTER_DEATH: true,
            },
          ],
          SHOOT_ON_DEATH: true,
        },
      });
    return output;
  })(),
};
