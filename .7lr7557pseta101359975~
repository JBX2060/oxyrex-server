let effectIndex = 0;

function createEffectBullet(type, stats = {}) {
  if (stats.time == null) {
    stats.time = 75;
  }
  if (stats.amplification == null) {
    stats.amplification = 1;
  }
  if (stats.parent == null) {
    stats.parent = exports.bullet;
  }
  exports[`effectBulletProp${effectIndex}`] = {
    SHAPE: stats.shape == null ? stats.parent.SHAPE || 0 : stats.shape,
  };
  const colors = {
    poison: 25,
    ice: 26,
    confus: 17,
    emp: 32,
    tesla: 33,
    fire: 34,
  };
  return {
    PARENT: [stats.parent],
    [type.toUpperCase()]: {
      STATUS: true,
      TIME: stats.time,
      AMPLIFY: stats.amplification,
    },
    TURRETS: [
      {
        POSITION: [25, 0, 0, 0, 360, 0],
        TYPE: [
          exports[type.toLowerCase() + "Hue"],
          {
            CAN_GO_OUTSIDE_ROOM: true,
          },
        ],
      },
      {
        POSITION: [10, 0, 0, 0, 0, 1],
        TYPE: [
          exports[`effectBulletProp${effectIndex++}`],
          {
            INDEPENDENT: true,
            COLOR: colors[type.toLowerCase()],
            CAN_GO_OUTSIDE_ROOM: true,
          },
        ],
      },
    ],
  };
}
exports.acidBullet = createEffectBullet("poison");
exports.weakAcidBullet = createEffectBullet("poison", {
  amplification: 0.7,
  time: 50,
});
exports.strongAcidBullet = createEffectBullet("poison", {
