exports.kilonova = {
  PARENT: [exports.genericTank],
  LABEL: "Kilonova",
  TOOLTIP: "Right-Click to detonate",
  GUNS: (function () {
    const output = [
      {
        POSITION: [18, 8, 1, 0, 0, 0, 0],
        PROPERTIES: {
          SHOOT_SETTINGS: combineStats([g.bullet]),
          TYPE: exports.bullet,
        },
      },
    ];
    for (let i = 0; i < 30; i++) {
      output.push({
        POSITION: [2, 2, 1, 4, 0, (i / 30) * 360, 0],
        PROPERTIES: {
          SHOOT_SETTINGS: combineStats([g.bullet, g.kiloring]),
          TYPE: exports.kiloshard,
          ALT_FIRE: true,
        },
      });
    }
    return output;
  })(),
  TURRETS: [
    {
      POSITION: [11, 0, 0, 0, 0, 1],
      TYPE: exports.blueTurret,
    },
    {
      /** SIZE     X       Y     ANGLE    ARC */
      POSITION: [28, 0, 0, 90, 360, 0],
      TYPE: exports.kiloRing,
    },
  ],
};
