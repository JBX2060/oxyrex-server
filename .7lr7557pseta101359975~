  PARENT: [exports.bullet],
  LABEL: "Nuke",
  SHAPE: shapeConfig.nuke,
  INDEPENDENT: true,
  BODY: {
    RANGE: 120,
  },
  MISSILE: true,
  GUNS: (() => {
    let output = [
      {
        POSITION: [1, 1, 1, 0, 0, 0, Infinity],
        PROPERTIES: {
          AUTOFIRE: true,
          SHOOT_SETTINGS: combineStats([g.bullet, g.explosion]),
          TYPE: [
            exports.explosion,
            {
              PERSISTS_AFTER_DEATH: true,
            },
          ],
          SHOOT_ON_DEATH: true,
        },
      },
    ];
    for (let i = 0; i < 8; i++) {
      output.push({
        POSITION: [1, 6, 1, 0, 0, (360 / 8) * i, Infinity],
        PROPERTIES: {
          AUTOFIRE: true,
          SHOOT_SETTINGS: combineStats([g.bullet, g.mach]),
          TYPE: [
            exports.shard,
            {
              PERSISTS_AFTER_DEATH: true,
            },
          ],
          SHOOT_ON_DEATH: true,
        },
      });
    }
    return output;
  })(),
};
