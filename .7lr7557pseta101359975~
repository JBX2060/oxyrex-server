exports.speedbumpMissile = {
  PARENT: [exports.bullet],
  LABEL: "Missile",
  INDEPENDENT: true,
  BODY: {
    RANGE: 120,
  },
  GUNS: (() => {
    let output = [];
    for (let i = 0; i < 6; i++) {
      output.push({
        POSITION: [50 - i * 5, 11, 1, 0, 0, 180, 1.75 + i / 6],
        PROPERTIES: {
          AUTOFIRE: true,
          SHOOT_SETTINGS: combineStats([
            g.bullet,
            g.missileTrail,
            g.rocketeerMissileTrail,
            g.speedbumpMissileTrail,
          ]),
          TYPE: [
            exports.bullet,
            {
              PERSISTS_AFTER_DEATH: true,
            },
          ],
          STAT_CALCULATOR: gunCalcNames.thruster,
          AUTOFIRE: true,
        },
      });
    }
    return output;
  })(),
};
