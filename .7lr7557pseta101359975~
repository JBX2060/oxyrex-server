exports.courser = {
  PARENT: [exports.genericTank],
  LABEL: "Courser",
  DANGER: 8,
  BODY: {
    FOV: 1.2,
    SPEED: base.SPEED * 0.7,
    ACCELERATION: base.ACCEL * 0.65,
  },
  GUNS: (function () {
    const output = [
      {
        POSITION: [15, 15, 1.2, 4, 0, 0, 0],
      },
    ];
    for (let i = 0; i < 50; i++) {
      output.push(
        {
          POSITION: [24, 5, 1, 0, i * 0.1, 0, i * 0.1],
          PROPERTIES: {
            SHOOT_SETTINGS: combineStats([g.bullet, g.course]),
            TYPE: exports.bullet,
          },
        },
        {
          POSITION: [24, 5, 1, 0, i * -0.1, 0, i * 0.1],
          PROPERTIES: {
            SHOOT_SETTINGS: combineStats([g.bullet, g.course]),
            TYPE: exports.bullet,
          },
        }
      );
    }
    return output.reverse();
  })(),
};
