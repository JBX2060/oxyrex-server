exports.foamBullet = {
  PARENT: [exports.bullet],
  LABEL: "Bubble",
  GUNS: (function () {
    const output = [];
    for (let i = 0; i < 12; i++) {
      output.push({
        POSITION: [
          1,
          1,
          1,
          0,
          0,
          (360 / 12) * i,
          0.75 + (i / 12) * Math.random(),
        ],
        PROPERTIES: {
          SHOOT_SETTINGS: combineStats([g.bullet, g.foamBullet]),
          TYPE: [
            exports.bullet,
            {
              PERSISTS_AFTER_DEATH: true,
              LAYER: 6,
            },
          ],
          AUTOFIRE: true,
        },
      });
    }
    return output;
  })(),
};
