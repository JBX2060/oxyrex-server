// SCHLIFFFENNNNNN
exports.scharnhorst = {
  PARENT: [exports.genericTank],
  LABEL: "Scharnhorst",
  SIZE: exports.genericTank.SIZE * 1.2,
  BODY: {
    HEALTH: base.HEALTH * 1.25,
    DAMAGE: base.DAMAGE * 1.175,
    SPEED: base.SPEED * 0.75,
    FOV: base.FOV * 1.075,
  },
  FACING_TYPE: "autospin",
  DANGER: 9,
  TURRETS: (function () {
    const output = [
      {
        POSITION: [27.5, 0, 0, 0, 360, 0],
        TYPE: exports.smasherBody,
      },
    ];
    g.scharnhorstAutoTurret = [
      1 + 0.0334 * 16,
      0.05,
      1.2,
      1,
      1 - 0.015 * 16,
      1 - 0.01 * 16,
      2,
      1.15,
      1.15,
      1.25,
      1,
      1,
      1,
    ];
    exports.scharnhorstAutoTurret = {
      PARENT: [exports.turretParent],
      LABEL: "Cannon",
      GUNS: [
        {
          POSITION: [24, 9, 1, 0, 0, 0, 0],
          PROPERTIES: {
            SHOOT_SETTINGS: combineStats([
              g.bullet,
              g.turret,
              g.scharnhorstAutoTurret,
            ]),
            TYPE: exports.bullet,
          },
        },
      ],
    };
    for (let i = 0; i < 16; i++) {
      output.push({
        POSITION: [
          6,
          i % 2 === 0 ? 8 : 9.5,
          0,
          (360 / 16) * i,
          90,
          +(i % 2 === 0),
        ],
        TYPE: [
          exports.scharnhorstAutoTurret,
          {
            COLOR: i % 4 === 0 ? 17 : 16,
          },
        ],
      });
    }
    return output;
  })(),
};
exports.schlieffen = {
  PARENT: [exports.genericTank],
  LABEL: "Schlieffen",
  SIZE: exports.genericTank.SIZE * 1.4,
  BODY: {
    HEALTH: base.HEALTH * 1.6,
    DAMAGE: base.DAMAGE * 1.3,
    SPEED: base.SPEED * 0.6,
    FOV: base.FOV * 1.125,
  },
  FACING_TYPE: "autospin",
  DANGER: 11,
  TURRETS: (function () {
    const output = [
      {
        POSITION: [27.5, 0, 0, 0, 360, 0],
        TYPE: exports.smasherBody,
      },
    ];
    g.schlieffenAutoTurret = [
      1 + 0.0334 * 20,
      0.05,
      1.2,
      1,
      1 - 0.015 * 20,
      1 - 0.01 * 20,
      2,
      1.15,
      1.15,
      1.25,
      1,
      1,
      1,
    ];
    exports.schlieffenAutoTurret = {
      PARENT: [exports.turretParent],
      LABEL: "Cannon",
      GUNS: [
        {
          POSITION: [24, 9, 1, 0, 0, 0, 0],
          PROPERTIES: {
            SHOOT_SETTINGS: combineStats([
              g.bullet,
              g.turret,
              g.schlieffenAutoTurret,
            ]),
            TYPE: exports.bullet,
          },
        },
      ],
    };
    for (let i = 0; i < 40; i++) {
      output.push({
        POSITION: [
          4.75,
          i % 2 === 0 ? 8 : 9.5,
          0,
          (360 / 16) * i,
          90,
          +(i % 2 === 0),
        ],
        TYPE: [
          exports.schlieffenAutoTurret,
          {
            COLOR: i % 3 === 0 ? 17 : 16,
          },
        ],
      });
    }
    return output;
  })(),
};
